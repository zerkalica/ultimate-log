{
	"logger.transport.ipc": {
		"class": "UltimateLogger.Transports.ipc",
		"options": {
			"rpcNamespace": "ultimate-logger"
		}
	},
	"logger.child": {
		"class": "UltimateLogger.Logger",
		"options": {
			"serialize": "@logger.serializer.node-serialize",
			"loggerSession": "@logger.session.default",
			"transports": [
				{
					"transport": "@logger.transport.ipc"
				}
			]
		}
	}
}
