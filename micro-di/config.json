{
	"ul": {
		"logDir": "",
		"errorLog": "@ul.logDir@/error.log",
		"debugLog": "@ul.logDir@/debug.log",
		"reopenSignal": "SIGHUP",
		"process-binder": {
			"@class": "UltimateLogger.ProcessBinder",
			"reopenSignal": "@ul.reopenSignal"
		},
		"listeners": [
			{
				"@class": "UltimateLogger.Listeners.std"
			},
			{
				"@class": "UltimateLogger.Listeners.ipc",
				"rpcNamespace": "ultimate-logger"
			}
		],
		"formatter": {
			"console": {
				"@class": "UltimateLogger.Formatters.TokenFormatter",
				"format": "[%type%][%id%]: %message%"
			},

			"file": {
				"@class": "UltimateLogger.Formatters.TokenFormatter",
				"format": "[%type%][%id%]: %message%"
			}
		},
		"filter": {
			"warn": {
				"@class": "UltimateLogger.Filters.type",
				"types": ["error", "warn"]
			},
			"debug": {
				"@class": "UltimateLogger.Filters.type",
				"types": ["info", "debug"]
			}
		},
		"child": {
			"@class": "UltimateLogger.Facades.Simple",
			"logger": {
				"@class": "UltimateLogger.Logger",
				"transports": {
					"ipc": {
						"@class": "UltimateLogger.Transports.ipc",
						"rpcNamespace": "ultimate-logger",
						"fallbackTransports": [
							{
								"@class": "UltimateLogger.Transports.console"
							}
						]
					}
				}
			}
		},

		"transports": {
			"console": {
				"@class": "UltimateLogger.Transports.console",
				"formatter": "@ul.formatter.console"
			},
			"file-error": {
				"@class": "UltimateLogger.Transports.file",
				"formatter": "@ul.formatter.file",
				"fileName": "@ul.errorLog",
				"filter": "@ul.filter.warn"
			},
			"file-debug": {
				"@class": "UltimateLogger.Transports.file",
				"formatter": "@ul.formatter.file",
				"fileName": "@ul.debugLog",
				"filter": "@ul.filter.debug"
			}
		},
		"logger": {
			"@class": "UltimateLogger.Logger",
			"processBinder": "@ul.process-binder",
			"listeners": "@ul.listeners",
			"transports": {
				"aggregator": {
					"@class": "UltimateLogger.Transports.aggregator",
					"transports": "@ul.transports"
				}
			}
		},

		"master": {
			"@class": "UltimateLogger.Facades.Simple",
			"logger": "@ul.logger"
		}
	}
}
