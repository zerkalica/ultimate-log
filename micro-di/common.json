{
	"logger.session.default": {
		"class": "UltimateLogger.LoggerSession"
	},

	"logger.formatter.token-formatter": {
		"class": "UltimateLogger.Formatters.TokenFormatter",
		"options": {
			"format": "[%type%][%id%]: %message%"
		}
	},

	"logger.process-binder": {
		"class": "UltimateLogger.ProcessBinder",
		"options": {
			"reopenSignal": "SIGHUP"
		}
	}
}
